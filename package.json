{
  "name": "dynamo-locx",
  "version": "1.0.0",
  "description": "Simple geospatial querying for Amazon DynamoDB",
  "scripts": {
    "prepublish": "tsc",
    "clean": "rimraf dist",
    "build": "tsc",
    "test": "ava test/**/*.ts"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Russell Steadman <code@steadman.email> (https://www.russellsteadman.com/)",
  "repository": "https://github.com/russellsteadman/dynamo-locx.git",
  "license": "Apache-2.0",
  "keywords": [
    "dynamodb",
    "geospatial",
    "geo",
    "location",
    "geoquery",
    "aws",
    "geohash",
    "geodata",
    "geojson",
    "geolocation"
  ],
  "dependencies": {
    "long": "^5.2.1",
    "nodes2ts": "^2.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.309.0",
    "ava": "^5.2.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.309.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "timeout": "600s"
  }
}
